CC = gcc
DEBUG = -g
CFLAGS = -Wall -c $(DEBUG)
LFLAGS = -Wall $(DEBUG)

sim01 : Sim01.o Structures.o Parser.o
	$(CC) $(LFLAGS) Sim01.o Structures.o Parser.o -o sim01

sim01.o : Sim01.c Sim01.h Structures.h Parser.h
	$(CC) $(CFLAGS) Sim01.c

structures.o : Structures.c Structures.h
	$(CC) $(CFLAGS) Structures.c

parser.o : Parser.c Parser.h
	$(CC) $(CFLAGS) Parser.c

clean:
	\rm *.o Sim01 *.stackdump 
