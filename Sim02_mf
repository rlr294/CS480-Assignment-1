CC = gcc
DEBUG = -g
CFLAGS = -Wall -c $(DEBUG)
LFLAGS = -Wall $(DEBUG)

Sim02 : Sim02.o Structures.o Parser.o
	$(CC) $(LFLAGS) Sim02.o Structures.o Parser.o -o Sim02

Sim02.o : Sim02.c Sim02.h Structures.h Parser.h
	$(CC) $(CFLAGS) Sim02.c

Structures.o : Structures.c Structures.h
	$(CC) $(CFLAGS) Structures.c

Parser.o : Parser.c Parser.h
	$(CC) $(CFLAGS) Parser.c

clean:
	\rm *.o Sim02 *.stackdump
